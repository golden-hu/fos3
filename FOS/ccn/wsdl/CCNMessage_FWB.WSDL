<?xml version="1.0"?>
<definitions xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:s="http://www.w3.org/2001/XMLSchema" xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xmlns:ccn="http://www.ccn.com.sg/CCNMSGWS" xmlns:tm="http://microsoft.com/wsdl/mime/textMatching/" xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:ns1="http://www.w3.org/2000/09/xmldsig#" targetNamespace="http://www.ccn.com.sg/CCNMSGWS">
	<types>
		<s:schema targetNamespace="http://www.ccn.com.sg/CCNMSGWS" elementFormDefault="unqualified" attributeFormDefault="unqualified">
			<s:annotation>
				<s:documentation>   
				</s:documentation>
			</s:annotation>
			<s:element name="CCN.MSG" type="ccn:CCN.MSG"/>
			<s:complexType name="CCN.MSG">
				<s:sequence>
					<s:element name="MsgEnvelope" type="ccn:Envelope"/>
					<s:element name="MsgBody" type="ccn:MsgBody"/>  
				</s:sequence>
			</s:complexType>
			<s:complexType name="Envelope">
				<s:sequence>
					<s:element name="Priority" type="ccn:PriorityBase" minOccurs="0">
						<s:annotation>
							<s:documentation>Priority Code</s:documentation>
						</s:annotation>
					</s:element>
					<s:element name="RecipientPimaAddress" type="ccn:PimaAddressBase"/>
					<s:element name="SenderPimaAddress" type="ccn:PimaAddressBase"/>
					<s:element name="GMT" type="ccn:GMTBase"/>
					<s:element name="SenderOriginalCode" type="ccn:SenderOriginalCodeBase"/>
					<s:element name="MessageSequenceNumber" type="ccn:InterchangeReferenceNumberBase"/>
				</s:sequence>
			</s:complexType>
			<s:simpleType name="PriorityBase">
				<s:restriction base="s:string">
					<s:pattern value="QK|QU|QP|QD"/>
				</s:restriction>
			</s:simpleType>
			<s:simpleType name="PimaAddressBase">
				<s:restriction base="s:string">
					<s:pattern value="[A-Z0-9/]{1,35}"/>
				</s:restriction>
			</s:simpleType>
			<s:simpleType name="SenderOriginalCodeBase">
				<s:restriction base="s:string">
					<s:pattern value="[A-Z0-9 /]{1,15}"/>
				</s:restriction>
			</s:simpleType>
			<s:simpleType name="TTYAddressBase">
				<s:restriction base="s:string">
					<s:pattern value="[A-Z0-9]{7}"/>
				</s:restriction>
			</s:simpleType>
			<s:simpleType name="GMTBase">
				<s:restriction base="s:string">
					<s:pattern value="[0-9]{6}"/>
				</s:restriction>
			</s:simpleType>
			<s:simpleType name="DoubleSignatureBase">
				<s:restriction base="s:string">
					<s:pattern value="[A-Z]{2}"/>
				</s:restriction>
			</s:simpleType>
			<s:simpleType name="InterchangeReferenceNumberBase">
				<s:restriction base="s:string">
					<s:pattern value="[A-Z0-9]{14}"/>
				</s:restriction>
			</s:simpleType>
			<s:complexType name="MsgBody">
				<s:sequence>
					<s:element name="FWB" type="ccn:FWB"/>
				</s:sequence>
			</s:complexType>
			<s:complexType name="FWB">
				<s:sequence>
				  <s:element name="StandardMessageIdentification" type="ccn:StandardMessageIdentification" />
				  <s:element name="ConsignmentDetail" type="ccn:ConsignmentDetail" />
				  <s:element name="FlightBooking" type="ccn:FlightBooking" />
				  <s:element name="Routing" type="ccn:Routing" />
				  <s:element name="Shipper" type="ccn:Shipper" />
				  <s:element name="Consignee" type="ccn:Consignee" />
				  <s:element name="Agent" type="ccn:Agent" />
				  <s:element name="SpecialServiceRequest" type="ccn:SSR" />
				  <s:element name="AlsoNotify" type="ccn:Notify" />
				  <s:element name="AccountingInformation" type="ccn:AccountInformation" />
				  <s:element name="ChargeDeclaration" type="ccn:ChargeDeclaration" />
				  <s:element name="RateDescription" type="ccn:RateDescription" />
				  <s:element name="OtherCharge" type="ccn:OtherCharge" />
				  <s:element name="PrepaidChargeSummary" type="ccn:PrepaidChargeSummary" />
				  <s:element name="CollectChargeSummary" type="ccn:CollectChargeSummary" />
				  <s:element name="ShipperCertification" type="ccn:ShipperCertification" />
				  <s:element name="CarrierExection" type="ccn:CarrierExection" />
				  <s:element name="OtherServiceInformation" type="ccn:OtherServiceInfo" />
				  <s:element name="CCChargeDestinationCurrency" type="ccn:CCChargeDestinationCurrency" />
				  <s:element name="SenderReference" type="ccn:SenderReference" />
				  <s:element name="CustomsOrigin" type="ccn:CustomsOrigin" />
				  <s:element name="CommissionInformation" type="ccn:CommissionInformation" />
				  <s:element name="SalesIncentiveInformation" type="ccn:SalesIncentive" />
				  <s:element name="AgentReferenceData" type="ccn:AgentReferenceData" />
				  <s:element name="SpecialHandlingData" type="ccn:SpecialHandlingData" />
				  <s:element name="NominatedHandlingParty" type="ccn:NominatedHandlingParty" />
				  <s:element name="ShipmentReferenceInformation" type="ccn:ShipmentReference" />
				  <s:element name="OtherParticipantInformation" type="ccn:OtherParticipantInfo" />
				  <s:element name="OtherCustomsInformation" type="ccn:OtherCustomsInfo" minOccurs="0" maxOccurs="99" />
				  <s:element name="AAIParticipantDetail" type="ccn:AAIParticipantDetail" />
				  <s:element name="AAITSPDetail" type="ccn:AAITSPDetail" />
				  <s:element name="AAIDimensionDetail" type="ccn:AAIDimensionDetail" />
				  <s:element name="AAICommodityDetail" type="ccn:AAICommodityDetail" />
				</s:sequence>
			  </s:complexType>
			  <!-- Consignment Detail -->
			  <s:complexType name="ConsignmentDetail">
				<s:sequence>
				  <s:element name="AWBIdentification" type="ccn:AWBIdentification" />
				  <s:element name="AWBOriginAndDestination" type="ccn:AWBOriginAndDestination" />
				  <s:element name="QuantityDetail" type="ccn:QuantityDetail" />
				  <s:element name="VolumeDetail" type="ccn:Volume" />
				  <s:element name="DensityGroup" type="ccn:DensityGroup" />
				</s:sequence>
			  </s:complexType>
			  <!-- Flight Booking -->
			  <s:complexType name="FlightBooking">
				<s:sequence>
				  <s:element name="FlightIdentification" type="ccn:FlightIdentification" maxOccurs="2" />
				</s:sequence>
			  </s:complexType>
			  <!--Routing -->
			  <s:complexType name="Routing">
				<s:sequence>
				  <s:element name="FirstDestination" type="ccn:FirstDestination" />
				  <s:element name="OnwardDestination" type="ccn:OnwardDestination" minOccurs="0" maxOccurs="2" />
				</s:sequence>
			  </s:complexType>
			  <!-- Shipper -->
			  <s:complexType name="Shipper">
				<s:sequence>
				  <s:element name="ShipperAccountDetail" type="ccn:AccountDetail" minOccurs="0" />
				  <s:element name="ShipperName" type="ccn:Name" />
				  <s:element name="ShipperStreetAddress" type="ccn:StreetAddress" />
				  <s:element name="ShipperLocation" type="ccn:Location" />
				  <s:element name="ShipperCodedLocation" type="ccn:CodedLocation" />
				  <s:element name="ShipperContactDetail" type="ccn:ContactDetail" minOccurs="0" maxOccurs="99" />
				</s:sequence>
			  </s:complexType>
			  <!-- Consignee -->
			  <s:complexType name="Consignee">
				<s:sequence>
				  <s:element name="ConsigneeAccountDetail" type="ccn:AccountDetail" minOccurs="0" />
				  <s:element name="ConsigneeName" type="ccn:Name" />
				  <s:element name="ConsigneeStreetAddress" type="ccn:StreetAddress" />
				  <s:element name="ConsigneeLocation" type="ccn:Location" />
				  <s:element name="ConsigneeCodedLocation" type="ccn:CodedLocation" />
				  <s:element name="ConsigneeContactDetail" type="ccn:ContactDetail" minOccurs="0" maxOccurs="99" />
				</s:sequence>
			  </s:complexType>
			  <!-- Agent Identification -->
			  <s:complexType name="Agent">
				<s:sequence>
				  <s:element name="AgentAccountDetail" type="ccn:AgtAccountDetail" />
				  <s:element name="AgentName" type="ccn:Name" />
				  <s:element name="AgentPlace" type="ccn:Place" />
				</s:sequence>
			  </s:complexType>
			  <!-- Special Service Request-->
			  <s:complexType name="SSR">
				<s:sequence>
				  <s:element name="SpecialServiceRequest1" type="ccn:SpecialServiceRequestBase" />
				  <s:element name="SpecialServiceRequest2" type="ccn:SpecialServiceRequestBase" minOccurs="0" />
				  <s:element name="SpecialServiceRequest3" type="ccn:SpecialServiceRequestBase" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!--Also Notify -->
			  <s:complexType name="Notify">
				<s:sequence>
				  <s:element name="NotifyName" type="ccn:Name" />
				  <s:element name="NotifyStreetAddress" type="ccn:StreetAddress" />
				  <s:element name="NotifyLocation" type="ccn:Location" />
				  <s:element name="NotifyCodedLocation" type="ccn:CodedLocation" />
				  <s:element name="NotifyContactDetail" type="ccn:ContactDetail" minOccurs="0" maxOccurs="99" />
				</s:sequence>
			  </s:complexType>
			  <!--Account Information-->
			  <s:complexType name="AccountInformation">
				<s:sequence>
				  <s:element name="AccountingDetail" type="ccn:AccountingDetail" maxOccurs="6" />
				</s:sequence>
			  </s:complexType>
			  <!--Charge Declaration-->
			  <s:complexType name="ChargeDeclaration">
				<s:sequence>
				  <s:element name="ISOCurrencyCode" type="ccn:ISOCurrencyCodeBase" />
				  <s:element name="ChargeCode" type="ccn:ChargeCodeBase" minOccurs="0" />
				  <s:element name="PCChargeDeclaration" type="ccn:PCChargeDeclaration" minOccurs="0" />
				  <s:element name="CarriageDeclaration" type="ccn:CarriageDeclaration" />
				  <s:element name="CustomDeclaration" type="ccn:CustomDeclaration" />
				  <s:element name="InsuranceDeclaration" type="ccn:InsuranceDeclaration" />
				</s:sequence>
			  </s:complexType>
			  <!--Rate Description-->
			  <s:complexType name="RateDescription">
				<s:sequence minOccurs="0" maxOccurs="11">
				  <s:element name="ChargeLineCount" type="ccn:ChargeLineCount" />
				</s:sequence>
			  </s:complexType>
			  <!--Other Charges-->
			  <s:complexType name="OtherCharge">
				<s:sequence minOccurs="0" maxOccurs="unbounded">
				  <s:element name="ChargeLine" type="ccn:ChargeLine" />
				</s:sequence>
			  </s:complexType>
			  <!--Prepaid Charge Summary-->
			  <s:complexType name="PrepaidChargeSummary">
				<s:sequence>
				  <s:element name="PrepaidTotalWeightCharge" type="ccn:TotalWeightCharge" />
				  <s:element name="PrepaidValuationCharge" type="ccn:ValuationCharge" />
				  <s:element name="PrepaidTaxes" type="ccn:Taxes" />
				  <s:element name="PrepaidTotalOthChargesDueAgent" type="ccn:OthChargeDueAgent" />
				  <s:element name="PrepaidTotalOthChargesDueCarrier" type="ccn:OthChargeDueCarrier" />
				  <s:element name="PrepaidChargeSummaryTotal" type="ccn:ChargeSummary" />
				</s:sequence>
			  </s:complexType>
			  <!--Collect Charge Summary-->
			  <s:complexType name="CollectChargeSummary">
				<s:sequence>
				  <s:element name="CollectTotalWeightCharge" type="ccn:TotalWeightCharge" />
				  <s:element name="CollectValuationCharge" type="ccn:ValuationCharge" />
				  <s:element name="CollectTaxes" type="ccn:Taxes" />
				  <s:element name="CollectTotalOthChargesDueAgent" type="ccn:OthChargeDueAgent" />
				  <s:element name="CollectTotalOthChargesDueCarrier" type="ccn:OthChargeDueCarrier" />
				  <s:element name="CollectChargeSummaryTotal" type="ccn:ChargeSummary" />
				</s:sequence>
			  </s:complexType>
			  <!--Shipper Certification-->
			  <s:complexType name="ShipperCertification">
				<s:sequence>
				  <s:element name="Signature" type="ccn:SignatureBase" />
				</s:sequence>
			  </s:complexType>
			  <!--Carrier Exection-->
			  <s:complexType name="CarrierExection">
				<s:sequence>
				  <s:element name="AWBIssueDetail" type="ccn:AWBIssueDetail" />
				  <s:element name="Authorisation" type="ccn:Authorisation" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!--Other Service Information-->
			  <s:complexType name="OtherServiceInfo">
				<s:sequence>
				  <s:element name="OtherServiceInformation1" type="ccn:OtherServiceInformationBase" />
				  <s:element name="OtherServiceInformation2" type="ccn:OtherServiceInformationBase" minOccurs="0" />
				  <s:element name="OtherServiceInformation3" type="ccn:OtherServiceInformationBase" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!--CC Charge in Dest currency-->
			  <s:complexType name="CCChargeDestinationCurrency">
				<s:sequence>
				  <s:element name="DestinationCurrency" type="ccn:DestinationCurrency" />
				  <s:element name="CurrencyConversionRate" type="ccn:CurrencyConversionRate" />
				  <s:element name="ChargeDestinationCurrency" type="ccn:ChargeDestinationCurrency" />
				  <s:element name="ChargeDestination" type="ccn:ChargeDestination" />
				  <s:element name="TotalCollectCharge" type="ccn:TotalCollectCharge" />
				</s:sequence>
			  </s:complexType>
			  <!--Sender Reference-->
			  <s:complexType name="SenderReference">
				<s:sequence>
				  <s:element name="SenderOfficeMessageAddress" type="ccn:SenderOfficeMessageAddress" minOccurs="0" />
				  <s:element name="SenderOfficeFileReference" type="ccn:SenderOfficeFileReference" minOccurs="0" />
				  <s:element name="SenderParticipantIdentification" type="ccn:SenderParticipantIdentification" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!--Customs Origin -->
			  <s:complexType name="CustomsOrigin">
				<s:sequence>
				  <s:element name="CustomsOriginCode" type="ccn:CustomsOriginCodeBase" />
				</s:sequence>
			  </s:complexType>
			  <!--Customs Origin -->
			  <s:complexType name="CommissionInformation">
				<s:sequence>
				  <s:element name="NoCommissionIndication" type="ccn:NoCommissionIndication" />
				  <s:element name="CommissionAmount" type="ccn:CommissionAmount" />
				  <s:element name="CommissionPercentage" type="ccn:CommissionPercentage" />
				</s:sequence>
			  </s:complexType>
			  <!--Sales Incentive Information-->
			  <s:complexType name="SalesIncentive">
				<s:sequence>
				  <s:element name="SalesIncentiveDetail" type="ccn:SalesIncentiveDetail" />
				  <s:element name="SalesIncentiveIndication" type="ccn:SalesIncentiveIndication" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!--Agent Reference Data-->
			  <s:complexType name="AgentReferenceData">
				<s:sequence>
				  <s:element name="AgentReference" type="ccn:AgentReference" />
				</s:sequence>
			  </s:complexType>
			  <!--Special Handlig Details-->
			  <s:complexType name="SpecialHandlingData">
				<s:sequence>
				  <s:element name="SpecialHandlingRequirement" type="ccn:SpecialHandlingRequirement" maxOccurs="9" />
				</s:sequence>
			  </s:complexType>
			  <!--Nominated Handling party -->
			  <s:complexType name="NominatedHandlingParty">
				<s:sequence>
				  <s:element name="Name" type="ccn:NameBase" />
				  <s:element name="Place" type="ccn:PlaceBase" />
				</s:sequence>
			  </s:complexType>
			  <!--Shipment Ref INfo -->
			  <s:complexType name="ShipmentReference">
				<s:sequence>
				  <s:element name="ReferenceNumber" type="ccn:ReferenceNumberBase" minOccurs="0" />
				  <s:element name="SupplementaryShipmentInfo1" type="ccn:SupplementaryShipmentInformationBase" minOccurs="0" />
				  <s:element name="SupplementaryShipmentInfo2" type="ccn:SupplementaryShipmentInformationBase" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!--Other Participant Information-->
			  <s:complexType name="OtherParticipantInfo">
				<s:sequence>
				  <s:element name="Name" type="ccn:NameBase" />
				  <s:element name="OtherParticipantOfficeMessage" type="ccn:OthParticipantOfficeMessage" minOccurs="0" />
				  <s:element name="OtherParticipantOfficeFileReference" type="ccn:OthParticipantOfficeFileReference" minOccurs="0" />
				  <s:element name="OtherParticipantIdentification" type="ccn:OthParticipantIdentification" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!--Other Customs Information-->
			  <s:complexType name="OtherCustomsInfo">
				<s:sequence>
				  <s:element name="ISOCountryCode" type="ccn:ISOCountryCodeBase" nillable="true" />
				  <s:element name="InformationIdentifier" type="ccn:LineIdentifierBase" minOccurs="0" nillable="true" />
				  <s:element name="CustomsInformationIdentifier" type="ccn:CustomsInformationIdentifierBase" minOccurs="0" nillable="true" />
				  <s:element name="SupplementCustomInformation" type="ccn:SupplementaryCustomsInformationBase" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!-- Level 2 -->
			  <!-- AWB Identification -->
			  <s:complexType name="AWBIdentification">
				<s:sequence>
				  <s:element name="AirlinePrefix" type="ccn:AirlinePrefixBase" />
				  <s:element name="AWBSerialNumber" type="ccn:AWBSerialNumberBase" />
				</s:sequence>
			  </s:complexType>
			  <!-- AWB Origin And Destination -->
			  <s:complexType name="AWBOriginAndDestination">
				<s:sequence>
				  <s:element name="AirportCodeOfOrigin" type="ccn:LocationIdentifierBase" />
				  <s:element name="AirportCodeOfDestination" type="ccn:LocationIdentifierBase" />
				</s:sequence>
			  </s:complexType>
			  <!-- Quantity Detail -->
			  <s:complexType name="QuantityDetail">
				<s:sequence>
				  <s:element name="ShipmentDescriptionCode" type="ccn:ShipmentDescriptionCodeBase" />
				  <s:element name="NumberOfPieces" type="ccn:NumberOfPiecesBase" />
				  <s:element name="WeightCode" type="ccn:WeightCodeBase" />
				  <s:element name="Weight" type="ccn:WeightBase" />
				</s:sequence>
			  </s:complexType>
			  <!--Flight Identification-->
			  <s:complexType name="FlightIdentification">
				<s:sequence>
				  <s:element name="CarrierCode" type="ccn:CarrierCodeBase" />
				  <s:element name="FlightNumber" type="ccn:FlightNumberBase" />
				  <s:element name="DayOfScheduledDeparture" type="ccn:DayOfMonthBase" />
				</s:sequence>
			  </s:complexType>
			  <!-- 1st Destination/Carrier-->
			  <s:complexType name="FirstDestination">
				<s:sequence>
				  <s:element name="AirportCityCode" type="ccn:LocationIdentifierBase" minOccurs="0" />
				  <s:element name="CarrierCode" type="ccn:CarrierCodeBase" />
				</s:sequence>
			  </s:complexType>
			  <!-- Onward Destination/Carrier-->
			  <s:complexType name="OnwardDestination">
				<s:sequence>
				  <s:element name="AirportCityCode" type="ccn:LocationIdentifierBase" />
				  <s:element name="CarrierCode" type="ccn:CarrierCodeBase" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!-- Account Detail -->
			  <s:complexType name="AccountDetail">
				<s:sequence>
				  <s:element name="AccountNumber" type="ccn:AccountNumberBase" />
				</s:sequence>
			  </s:complexType>
			  <!-- Name -->
			  <s:complexType name="Name">
				<s:sequence>
				  <s:element name="Name" type="ccn:NameBase" />
				</s:sequence>
			  </s:complexType>
			  <!-- Street Address -->
			  <s:complexType name="StreetAddress">
				<s:sequence>
				  <s:element name="StreetAddress" type="ccn:StreetAddressBase" />
				</s:sequence>
			  </s:complexType>
			  <!-- Location -->
			  <s:complexType name="Location">
				<s:sequence>
				  <s:element name="Place" type="ccn:PlaceBase" />
				  <s:element name="StateProvince" type="ccn:StateProvinceBase" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!-- Coded Location -->
			  <s:complexType name="CodedLocation">
				<s:sequence>
				  <s:element name="ISOCountryCode" type="ccn:ISOCountryCodeBase" />
				  <s:element name="PostCode" type="ccn:PostCodeBase" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!-- Contact Detail -->
			  <s:complexType name="ContactDetail">
				<s:sequence>
				  <s:element name="ContactIdentifier" type="ccn:ContactIdentifierBase" />
				  <s:element name="ContactNumber" type="ccn:ContactNumberBase" />
				</s:sequence>
			  </s:complexType>
			  <!-- Place -->
			  <s:complexType name="Place">
				<s:sequence>
				  <s:element name="Place" type="ccn:PlaceBase" />
				</s:sequence>
			  </s:complexType>
			  <!--Agent Identification Account Detail -->
			  <s:complexType name="AgtAccountDetail">
				<s:sequence>
				  <s:element name="AccountNumber" type="ccn:AccountNumberBase" minOccurs="0" />
				  <s:element name="AgentNumericCode" type="ccn:IATACargoAgentNumericCodeBase" />
				  <s:element name="AgentCASSAddress" type="ccn:IATACargoAgentCASSAddressBase" minOccurs="0" />
				  <s:element name="ParticipantIdentifier" type="ccn:ParticipantIdentifierBase" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!--Accounting Information -->
			  <s:complexType name="AccountingDetail">
				<s:sequence>
				  <s:element name="AccountInformationIdentifier" type="ccn:AccountingInformationIdentifierBase" />
				  <s:element name="AccountInformation" type="ccn:AccountingInformationBase" />
				</s:sequence>
			  </s:complexType>
			  <!--Prepaid / Collect ChargeInformation -->
			  <s:complexType name="PCChargeDeclaration">
				<s:sequence>
				  <s:element name="WeightValuation" type="ccn:PCIndBase" />
				  <s:element name="OtherCharges" type="ccn:PCIndBase" />
				</s:sequence>
			  </s:complexType>
			  <!--Value Carriage Declaration-->
			<s:complexType name="CarriageDeclaration">
				<s:sequence>
						<s:element name="DeclaredValueCarriage" type="ccn:DeclaredValueForCarriageBase" minOccurs="0"/>
						<s:element name="NoValueDeclared" type="ccn:DeclaredValueForCarriageBase" minOccurs="0"/>
				</s:sequence>
			</s:complexType>
			  <!--Value Custom Declaration-->
			  <s:complexType name="CustomDeclaration">
				<s:sequence>
				  <s:element name="DeclaredValueCustoms" type="ccn:DeclaredValueForCustomsBase" minOccurs="0" />
				  <s:element name="NoCustomValue" type="ccn:DeclaredValueForCustomsBase" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!--Value Insurance Declaration-->
			  <s:complexType name="InsuranceDeclaration">
				<s:sequence>
				  <s:element name="AmountInsurance" type="ccn:AmountOfInsuranceBase" minOccurs="0" />
				  <s:element name="NoValue" type="ccn:AmountOfInsuranceBase" minOccurs="0" />
				</s:sequence>
			  </s:complexType>
			  <!--Charge Line Count -->
			  <s:complexType name="ChargeLineCount">
				<s:sequence>
				  <s:element name="AWBRateLineNumber" type="ccn:AWBRateLineNumberBase" />
