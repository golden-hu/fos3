<definitions xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:s="http://www.w3.org/2001/XMLSchema" xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xmlns:ccn="http://www.ccn.com.sg/CCNMSGWS" xmlns:tm="http://microsoft.com/wsdl/mime/textMatching/" xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:ns1="http://www.w3.org/2000/09/xmldsig#" targetNamespace="http://www.ccn.com.sg/CCNMSGWS">
	<types>
		<s:schema targetNamespace="http://www.ccn.com.sg/CCNMSGWS" elementFormDefault="unqualified" attributeFormDefault="unqualified">
		<s:element name="CCN.MSG" type="ccn:CCN.MSG"/>
			<s:complexType name="CCN.MSG">
				<s:sequence>
					<s:element name="MsgEnvelope" type="ccn:Envelope"/>
					<s:element name="MsgBody" type="ccn:MsgBody"/>
				</s:sequence>
			</s:complexType>
			<s:complexType name="Envelope">
				<s:sequence>
					<s:element name="Priority" type="ccn:PriorityBase" minOccurs="0">
						<s:annotation>
							<s:documentation>Priority Code</s:documentation>
						</s:annotation>
					</s:element>
					<s:element name="RecipientPimaAddress" type="ccn:PimaAddressBase"/>
					<s:element name="SenderPimaAddress" type="ccn:PimaAddressBase"/>
					<s:element name="GMT" type="ccn:GMTBase"/>
					<s:element name="SenderOriginalCode" type="ccn:SenderOriginalCodeBase"/>
					<s:element name="MessageSequenceNumber" type="ccn:InterchangeReferenceNumberBase"/>
				</s:sequence>
			</s:complexType>
			<s:simpleType name="PriorityBase">
				<s:restriction base="s:string">
					<s:pattern value="QK|QU|QP|QD"/>
				</s:restriction>
			</s:simpleType>
			<s:simpleType name="PimaAddressBase">
				<s:restriction base="s:string">
					<s:pattern value="[A-Z0-9/]{1,35}"/>
				</s:restriction>
			</s:simpleType>
			<s:simpleType name="SenderOriginalCodeBase">
				<s:restriction base="s:string">
					<s:pattern value="[A-Z0-9 /]{1,15}"/>
				</s:restriction>
			</s:simpleType>
			<s:simpleType name="TTYAddressBase">
				<s:restriction base="s:string">
					<s:pattern value="[A-Z0-9]{7}"/>
				</s:restriction>
			</s:simpleType>
			<s:simpleType name="GMTBase">
				<s:restriction base="s:string">
					<s:pattern value="[0-9]{6}"/>
				</s:restriction>
			</s:simpleType>
			<s:simpleType name="DoubleSignatureBase">
				<s:restriction base="s:string">
					<s:pattern value="[A-Z]{2}"/>
				</s:restriction>
			</s:simpleType>
			<s:simpleType name="InterchangeReferenceNumberBase">
				<s:restriction base="s:string">
					<s:pattern value="[A-Z0-9]{14}"/>
				</s:restriction>
			</s:simpleType>
			<s:complexType name="MsgBody">
				<s:sequence>
					<s:element name="FHL" type="ccn:FHL"/>
				</s:sequence>
			</s:complexType>
			<s:complexType name="FHL">
				<s:sequence>
					<s:element name="StandardMessageIdentification" type="ccn:StandardMessageIdentification" />
					<s:element name="MasterAWBConsignmentDetail" type="ccn:MasterAWBConsignmentDetail" />
					<s:element name="HouseAndFreeTextGroup" type="ccn:HouseAndFreeTextGroup" />
					<s:element name="ShipperNameAndAddress" type="ccn:ShipperNameAndAddress" />
					<s:element name="ConsigneeNameAndAddress" type="ccn:ConsigneeNameAndAddress" />
					<s:element name="ChargeDeclarations" type="ccn:ChargeDeclarations" />
					<s:element name="AAIParticipantDetail" type="ccn:ParticipantDetail" minOccurs="0" />
					<s:element name="AAITSPDetail" type="ccn:TSPDetail" minOccurs="0" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="StandardMessageIdentification">			
			    <s:sequence>
			      <s:element name="StandardMessageIdentification" type="ccn:StandardMessageIdentifierBase" />
			      <s:element name="MessageTypeVersionNumber" type="ccn:MessageSequenceNumberBase" />			      
			    </s:sequence>
			  </s:complexType>
			<!-- Grouping -->
			<s:complexType name="HouseAndFreeTextGroup">
				<s:sequence>
					<s:element name="HouseWaybillSummaryDetail" type="ccn:HouseWaybillSummaryDetail" />
					<s:element name="FreeTextDescriptionOfGoods" type="ccn:FreeTextDescriptionOfGoods" minOccurs="0" maxOccurs="9" />
					<s:element name="HarmonisedTariffScheduleInformation" type="ccn:HarmonisedTariffScheduleInformation" minOccurs="0" maxOccurs="9" />
					<s:element name="OtherCustomsInfo" type="ccn:OtherCustomsInfo" minOccurs="0" maxOccurs="99" />
				</s:sequence>
			</s:complexType>
			<!-- Level 1 -->
			<s:complexType name="MasterAWBConsignmentDetail">
				<s:sequence>
					<s:element name="MasterAWBIdentification" type="ccn:MasterAWBIdentification" />
					<s:element name="AWBOriginAndDestination" type="ccn:AWBOriginAndDestination" />
					<s:element name="QuantityDetail" type="ccn:QuantityDetail" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="HouseWaybillSummaryDetail">
				<s:sequence>
					<s:element name="HWBSerialNumber" type="ccn:HWBSerialNumberBase" />
					<s:element name="HouseWaybillOriginAndDestination" type="ccn:HouseWaybillOriginAndDestination" />
					<s:element name="HouseWaybillTotals" type="ccn:HouseWaybillTotals" />
					<s:element name="NatureOfGoods" type="ccn:NatureOfGoods" />
					<s:element name="SpecialHandlingRequirements" type="ccn:SpecialHandlingRequirements" minOccurs="0" maxOccurs="9" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="FreeTextDescriptionOfGoods">
				<s:sequence>
					<s:element name="FreeText" type="ccn:FreeTextBase" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="ShipperNameAndAddress">
				<s:sequence>
					<s:element name="SHPName" type="ccn:NameBase" />
					<s:element name="SHPStreetAddress" type="ccn:StreetAddress" />
					<s:element name="SHPLocation" type="ccn:Location" />
					<s:element name="SHPCodedLocation" type="ccn:CodedLocation" />
					<s:element name="SHPContactDetail" type="ccn:ContactDetail" minOccurs="0" maxOccurs="99"/>
				</s:sequence>
			</s:complexType>
			<s:complexType name="ConsigneeNameAndAddress">
				<s:sequence>
					<s:element name="CNEName" type="ccn:NameBase" />
					<s:element name="CNEStreetAddress" type="ccn:StreetAddress" />
					<s:element name="CNELocation" type="ccn:Location" />
					<s:element name="CNECodedLocation" type="ccn:CodedLocation" />
					<s:element name="CNEContactDetail" type="ccn:ContactDetail" minOccurs="0" maxOccurs="99"/>
				</s:sequence>
			</s:complexType>
			<s:complexType name="ChargeDeclarations">
				<s:sequence>
					<s:element name="ISOCurrencyCode" type="ccn:ISOCurrencyCodeBase" />
					<s:element name="PrepaidCollectChargeDeclarations" type="ccn:PrepaidCollectChargeDeclarations" />
					<s:element name="ValueForCarriageDeclaration" type="ccn:ValueForCarriageDeclaration" />
					<s:element name="ValueForCustomsDeclaration" type="ccn:ValueForCustomsDeclaration" />
					<s:element name="ValueForInsuranceDeclaration" type="ccn:ValueForInsuranceDeclaration" />
				</s:sequence>
			</s:complexType>
			<!-- Level 2 -->
			<s:complexType name="MasterAWBIdentification">
				<s:sequence>
					<s:element name="AirlinePrefix" type="ccn:AirlinePrefixBase" />
					<s:element name="AWBSerialNumber" type="ccn:AWBSerialNumberBase" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="AWBOriginAndDestination">
				<s:sequence>
					<s:element name="AirportccnyCodeOfOrigin" type="ccn:LocationIdentifierBase" />
					<s:element name="AirportccnyCodeOfDestination" type="ccn:LocationIdentifierBase" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="QuantityDetail">
				<s:sequence>
					<s:element name="ShipmentDescriptionCode" type="ccn:ShipmentDescriptionCodeBase" />
					<s:element name="NumberOfPieces" type="ccn:NumberOfPiecesBase" />
					<s:element name="WeightCode" type="ccn:WeightCodeBase" />
					<s:element name="Weight" type="ccn:WeightBase" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="HouseWaybillOriginAndDestination">
				<s:sequence>
					<s:element name="AirportccnyCodeOfDeparture" type="ccn:LocationIdentifierBase" />
					<s:element name="AirportccnyCodeOfDestination" type="ccn:LocationIdentifierBase" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="HouseWaybillTotals">
				<s:sequence>
					<s:element name="NumberOfPieces" type="ccn:NumberOfPiecesBase" />
					<s:element name="WeightCode" type="ccn:WeightCodeBase" />
					<s:element name="Weight" type="ccn:WeightBase" />
					<s:element name="SLAC" type="ccn:SLACBase" nillable="true" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="NatureOfGoods">
				<s:sequence>
					<s:element name="ManifestDescriptionOfGoods" type="ccn:ManifestDescriptionOfGoodsBase" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="SpecialHandlingRequirements">
				<s:sequence>
					<s:element name="SpecialHandlingCode" type="ccn:SpecialHandlingCodeBase" />					
				</s:sequence>
			</s:complexType>
			<s:complexType name="StreetAddress">
				<s:sequence>
					<s:element name="StreetAddress" type="ccn:StreetAddressBase" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="Location">
				<s:sequence>
					<s:element name="Place" type="ccn:PlaceBase" />
					<s:element name="StateProvince" type="ccn:StateProvinceBase" minOccurs="0" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="CodedLocation">
				<s:sequence>
					<s:element name="ISOCountryCode" type="ccn:ISOCountryCodeBase" />
					<s:element name="PostCode" type="ccn:PostCodeBase" minOccurs="0" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="ContactDetail">
				<s:sequence>
					<s:element name="ContactIdentifier" type="ccn:ContactIdentifierBase" />
					<s:element name="ContactNumber" type="ccn:ContactNumberBase" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="PrepaidCollectChargeDeclarations">
				<s:sequence minOccurs="0">
					<s:element name="PCIndWeightValuation" type="ccn:PCIndBase" />
					<s:element name="PCIndOtherCharges" type="ccn:PCIndBase" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="ValueForCarriageDeclaration">
				<s:sequence minOccurs="0">
					<s:element name="DeclaredValueForCarriage" type="ccn:DeclaredValueForCarriageBase" minOccurs="0" nillable="true" />
					<s:element name="NoValueDeclared" type="ccn:DeclaredValueForCarriageBase" minOccurs="0" nillable="true" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="ValueForCustomsDeclaration">
				<s:sequence minOccurs="0">
					<s:element name="DeclaredValueForCustoms" type="ccn:DeclaredValueForCustomsBase" minOccurs="0" nillable="true" />
					<s:element name="NoCustomsValue" type="ccn:DeclaredValueForCustomsBase" minOccurs="0" nillable="true" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="ValueForInsuranceDeclaration">
				<s:sequence minOccurs="0">
					<s:element name="DeclaredValueForInsurance" type="ccn:AmountOfInsuranceBase" minOccurs="0" nillable="true" />
					<s:element name="NoValue" type="ccn:AmountOfInsuranceBase" minOccurs="0" nillable="true" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="ParticipantDetail">
				<s:sequence>
					<s:element name="RecipientOfMessage" type="ccn:RecipientOfMessageBase" />
					<s:element name="CarrierCodeBased" type="ccn:CarrierCodeBase" />
					<s:element name="TotalChargeableWeight" type="ccn:WeightBase" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="TSPDetail">
				<s:sequence>
					<s:element name="ShippingBillNumber" type="ccn:AAIShippingBillNumber" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="HarmonisedTariffScheduleInformation">
				<s:sequence>
					<s:element name="HarmonisedCommodityCode" type="ccn:HarmonisedCommodityCodeBase" />
				</s:sequence>
			</s:complexType>
			<s:complexType name="OtherCustomsInfo">
				<s:sequence>
					<s:element name="ISOCountryCode" type="ccn:ISOCountryCodeBase" nillable="true" />
					<s:element name="InformationIdentifier" type="ccn:LineIdentifierBase" nillable="true" />
					<s:element name="CustomsInformationIdentifier" type="ccn:CustomsInformationIdentifierBase" nillable="true" />
					<s:element name="SupplementCustomInformation" type="ccn:SupplementaryCustomsInformationBase" />
				</s:sequence>
			</s:complexType>
		
			
