<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CNNWebServiceService WSDL service -->

  <service name="CNNWebService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ccn.com.sg"/>
      <parameter name="wsdlServiceElement" value="CNNWebServiceService"/>
      <parameter name="schemaQualified" value="http://xml.apache.org/xml-soap,http://ccn.com.sg"/>
      <parameter name="wsdlServicePort" value="CNNWebService"/>
      <parameter name="className" value="sg.com.ccn.CNNWebServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CNNWebService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="activate" qname="operNS:Activate" xmlns:operNS="http://ccn.com.sg" returnQName="retNS:ActivateReturn" xmlns:retNS="http://ccn.com.sg" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:accountID" xmlns:pns="http://ccn.com.sg" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:keyValuePair" xmlns:pns="http://ccn.com.sg" type="tns:Map" xmlns:tns="http://xml.apache.org/xml-soap"/>
      </operation>
      <operation name="suspend" qname="operNS:Suspend" xmlns:operNS="http://ccn.com.sg" returnQName="retNS:SuspendReturn" xmlns:retNS="http://ccn.com.sg" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:accountID" xmlns:pns="http://ccn.com.sg" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:keyValuePair" xmlns:pns="http://ccn.com.sg" type="tns:Map" xmlns:tns="http://xml.apache.org/xml-soap"/>
      </operation>
      <operation name="unSuspend" qname="operNS:UnSuspend" xmlns:operNS="http://ccn.com.sg" returnQName="retNS:UnSuspendReturn" xmlns:retNS="http://ccn.com.sg" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:accountID" xmlns:pns="http://ccn.com.sg" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:keyValuePair" xmlns:pns="http://ccn.com.sg" type="tns:Map" xmlns:tns="http://xml.apache.org/xml-soap"/>
      </operation>
      <operation name="terminate" qname="operNS:Terminate" xmlns:operNS="http://ccn.com.sg" returnQName="retNS:TerminateReturn" xmlns:retNS="http://ccn.com.sg" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:accountID" xmlns:pns="http://ccn.com.sg" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:keyValuePair" xmlns:pns="http://ccn.com.sg" type="tns:Map" xmlns:tns="http://xml.apache.org/xml-soap"/>
      </operation>
      <parameter name="allowedMethods" value="suspend activate terminate unSuspend"/>

  </service>
</deployment>
