<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CPProductServiceService WSDL service -->

  <service name="CPProductService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://impl.service.ccn.com.sg"/>
      <parameter name="wsdlServiceElement" value="CPProductServiceService"/>
      <parameter name="schemaQualified" value="http://xml.apache.org/xml-soap,http://impl.service.ccn.com.sg"/>
      <parameter name="wsdlServicePort" value="CPProductService"/>
      <parameter name="className" value="sg.com.ccn.service.impl.CPProductServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CPProductService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="activate" qname="operNS:Activate" xmlns:operNS="http://impl.service.ccn.com.sg" soapAction="" >
        <parameter qname="pns:accountId" xmlns:pns="http://impl.service.ccn.com.sg" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:keyValuePair" xmlns:pns="http://impl.service.ccn.com.sg" type="tns:Map" xmlns:tns="http://xml.apache.org/xml-soap"/>
      </operation>
      <operation name="suspend" qname="operNS:Suspend" xmlns:operNS="http://impl.service.ccn.com.sg" soapAction="" >
        <parameter qname="pns:accountId" xmlns:pns="http://impl.service.ccn.com.sg" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:keyValuePair" xmlns:pns="http://impl.service.ccn.com.sg" type="tns:Map" xmlns:tns="http://xml.apache.org/xml-soap"/>
      </operation>
      <operation name="unSuspend" qname="operNS:UnSuspend" xmlns:operNS="http://impl.service.ccn.com.sg" soapAction="" >
        <parameter qname="pns:accountId" xmlns:pns="http://impl.service.ccn.com.sg" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:keyValuePair" xmlns:pns="http://impl.service.ccn.com.sg" type="tns:Map" xmlns:tns="http://xml.apache.org/xml-soap"/>
      </operation>
      <operation name="terminate" qname="operNS:Terminate" xmlns:operNS="http://impl.service.ccn.com.sg" soapAction="" >
        <parameter qname="pns:accountId" xmlns:pns="http://impl.service.ccn.com.sg" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:keyValuePair" xmlns:pns="http://impl.service.ccn.com.sg" type="tns:Map" xmlns:tns="http://xml.apache.org/xml-soap"/>
      </operation>
      <operation name="validateAccess" qname="operNS:ValidateAccess" xmlns:operNS="http://impl.service.ccn.com.sg" soapAction="" >
        <parameter qname="pns:accountId" xmlns:pns="http://impl.service.ccn.com.sg" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:keyValuePair" xmlns:pns="http://impl.service.ccn.com.sg" type="tns:Map" xmlns:tns="http://xml.apache.org/xml-soap"/>
      </operation>
      <parameter name="allowedMethods" value="suspend activate terminate unSuspend validateAccess"/>

  </service>
</deployment>
